{
  "name": "react-material-ui-starter",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "scripts": {
    "build": "browserify main.jsx > public/bundle.js",
    "build-prod": "NODE_ENV=production browserify main.jsx | uglifyjs -cm > public/bundle.js",
    "start": "live-server",
    "watch": "watchify main.jsx -o public/bundle.js -dv"
  },
  "dependencies": {
    "babelify": "^6.1.3",
    "browserify": "^10.2.6",
    "material-ui": "^0.13.4",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-tap-event-plugin": "^0.2.1",
    "uglify-js": "^2.4.24",
    "watchify": "^3.2.3"
  }
}
